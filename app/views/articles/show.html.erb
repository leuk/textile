<% title "Article" %>

<p>
  <strong>Title:</strong>
  <%=h @article.title %>
</p>
<p>
  <strong>Content:</strong>
  <%=h @article.content %>
</p>
<p>
  <strong>S Publish:</strong>
  <%=h @article.s_publish %>
</p>
<p>
  <strong>E Publish:</strong>
  <%=h @article.e_publish %>
</p>
<p>
  <strong>Page Title:</strong>
  <%=h @article.page_title %>
</p>
<p>
  <strong>Keywords:</strong>
  <%=h @article.keywords %>
</p>
<p>
  <strong>Meta:</strong>
  <%=h @article.meta %>
</p>

<p>
  <%= link_to "Edit", edit_article_path(@article) %> |
  <%= link_to "Destroy", @article, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", articles_path %>
</p>
